Xout = setnames(Xout, old = names(Xout[3:length(Xout)]), new = new_col_names)
#Store the final data frame as a text file using write.table(), with
#row.names = FALSE:
write.table(Xout, file = "Xout.txt", row.names = FALSE, quote = FALSE,
fileEncoding = "UTF8")
